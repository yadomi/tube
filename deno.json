{
  "fmt": {
    "lineWidth": 200,
    "exclude": ["./data", "./public"]
  },
  "tasks": {
    "run": "deno run  --allow-env --allow-read --allow-write --allow-net --unstable-kv --unstable-cron main.ts",
    "lock": "deno cache --lock=deno.lock --lock-write main.ts",
    "build": "docker build -t yadomi/tube ."
  }
}
